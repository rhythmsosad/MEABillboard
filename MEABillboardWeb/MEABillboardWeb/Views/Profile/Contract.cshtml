<!--=== Profile ===-->
<div class="container content profile">
    <div class="row">
        <!--Left Sidebar-->
        <div class="col-md-3 md-margin-bottom-40">
            @*<img class="img-responsive profile-img margin-bottom-20" src="assets/img/team/img1-md.jpg" alt="">*@


            <ul class="list-group sidebar-nav-v1 margin-bottom-40" id="sidebar-nav-1">
                <li class="list-group-item">
                    <a href="@Url.Action("Index")"><i class="fa fa-user"></i> ข้อมูลส่วนตัว</a>
                </li>

                <li class="list-group-item">
                    <a href="@Url.Action("Contract")"><i class="fa fa-cubes"></i> สัญญา</a>
                </li>

                <li class="list-group-item active">
                    <a href="#"><i class="fa fa-cog"></i> Settings</a>
                </li>
            </ul>

            <div class="panel-heading-v2 overflow-h">
                <h2 class="heading-xs pull-left"><i class="fa fa-bar-chart-o"></i> Task Progress</h2>
                <a href="#"><i class="fa fa-cog pull-right"></i></a>
            </div>
            <h3 class="heading-xs">Web Design <span class="pull-right">92%</span></h3>
            <div class="progress progress-u progress-xxs">
                <div style="width: 92%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="92" role="progressbar" class="progress-bar progress-bar-u">
                </div>
            </div>
            <h3 class="heading-xs">Unify Project <span class="pull-right">85%</span></h3>
            <div class="progress progress-u progress-xxs">
                <div style="width: 85%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="85" role="progressbar" class="progress-bar progress-bar-blue">
                </div>
            </div>
            <h3 class="heading-xs">Sony Corporation <span class="pull-right">64%</span></h3>
            <div class="progress progress-u progress-xxs margin-bottom-40">
                <div style="width: 64%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="64" role="progressbar" class="progress-bar progress-bar-dark">
                </div>
            </div>
        </div>
        <!--End Left Sidebar-->

        <!-- Profile Content -->
        <div class="col-md-9">
            <div class="headline"><h2>ค้นหา</h2></div>
            <form action="@Url.Action("Index")" id="sky-form4" class="sky-form" novalidate="novalidate" method="post">
                <fieldset>
                    <section>
                        <label class="input">
                            <input type="text" name="ContractName" placeholder="ชื่อโครงการ">
                            <b class="tooltip tooltip-bottom-right">ชื่อโครงการ</b>
                        </label>
                    </section>
                    <section>
                        <label class="label">ประเภท</label>
                        <div class="inline-group">
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline"><i></i>type 1</label>
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline"><i></i>type 2</label>
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline"><i></i>type 3</label>
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline"><i></i>type 4</label>
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline"><i></i>type 5</label>
                        </div>
                    </section>
                    <section>
                        <label class="label">สถานะ</label>
                        <div class="inline-group">
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline" ><i></i>status 1</label>
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline"><i></i>status 2</label>
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline"><i></i>status 3</label>
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline"><i></i>status 4</label>
                            <label class="checkbox"><input type="checkbox" name="checkbox-inline"><i></i>status 5</label>
                        </div>
                    </section>

                    <button type="submit" class="btn btn-u">ค้นหา</button>
                </fieldset>

               
               
            </form>


            <div class="margin-bottom-60"></div>
            <div class="headline"><h2>รายการสัญญา</h2></div>
            <div class="table-search-v1 panel panel-dark margin-bottom-50">
               
                <div class="table-responsive">
                    <table class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>ชื่อ</th>
                                <th>วันที่</th>
                                <th>ประเภท</th>
                                <th>สถานะ</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <a href="@Url.Action("ContractInfo")">HP Enterprise Service</a>
                                </td>
                                <td>
                                   1 ม.ค. 2557 - 15 ม.ค. 2557
                                </td>
                                <td>
                                   สื่อ digital
                                </td>                               
                                <td>
                                    <span class="label label-success">&nbsp;&nbsp;&nbsp;&nbsp;ผ่าน&nbsp;&nbsp;&nbsp;&nbsp; </span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <a href="#">Samsung Electronics</a>                                   
                                </td>
                                <td>
                                    1 ม.ค. 2557 - 15 ม.ค. 2557
                                </td>
                                <td>
                                    สื่อ digital
                                </td>
                                <td>
                                    <span class="label label-success">&nbsp;&nbsp;&nbsp;&nbsp;ผ่าน&nbsp;&nbsp;&nbsp;&nbsp;</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <a href="#">Sony Corporation</a>                                    
                                </td>
                                <td>
                                    1 ม.ค. 2557 - 15 ม.ค. 2557
                                </td>
                                <td>
                                    ป้าย
                                </td>
                                <td>
                                    <span class="label label-danger">&nbsp;หมดอายุ&nbsp;</span>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <a href="#">Lenovo Group</a>                                   
                                </td>
                                <td>
                                    1 ม.ค. 2557 - 15 ม.ค. 2557
                                </td>
                                <td>
                                    ป้าย
                                </td>
                                <td>
                                    <span class="label label-yellow">กำลังตรวจสอบ</span>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- End Profile Content -->
    </div><!--/end row-->
</div>
